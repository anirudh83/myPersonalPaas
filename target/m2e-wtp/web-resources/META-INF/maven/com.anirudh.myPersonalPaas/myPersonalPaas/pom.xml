  <artifactId>myPersonalPaas</artifactId>
  <packaging>war</packaging>
  <version>1.0-SNAPSHOT</version>
  <name>myPersonalPaas Maven Webapp</name>
  <url>http://maven.apache.org</url>
  <properties>
